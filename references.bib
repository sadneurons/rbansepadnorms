@article{RN35,
   author = {Claes, A. J. and Mertens, G. and Gilles, A. and Hofkens-Van den Brandt, A. and Fransen, E. and Van Rompaey, V. and Van de Heyning, P.},
   title = {The Repeatable Battery for the Assessment of Neuropsychological Status for Hearing Impaired Individuals (RBANS-H) before and after Cochlear Implantation: A Protocol for a Prospective, Longitudinal Cohort Study},
   journal = {Front Neurosci},
   volume = {10},
   pages = {512},
   note = {Claes, Annes J
Mertens, Griet
Gilles, Annick
Hofkens-Van den Brandt, Anouk
